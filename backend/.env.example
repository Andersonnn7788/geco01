# Infinity8 Booking Agent Backend - Environment Variables

# OpenAI
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini

# Supabase
SUPABASE_URL=https://xxx.supabase.co
SUPABASE_SERVICE_KEY=eyJ...  # Service role key (NOT anon key)

# Stripe (Test Mode)
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PUBLISHABLE_KEY=pk_test_...

# Frontend
FRONTEND_URL=http://localhost:3000

# Email (Resend)
RESEND_API_KEY=re_...
RESEND_FROM=Infinity8 <bookings@yourdomain.com>

# Google Maps (Street View)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_maps_api_key

# SMTP (optional fallback; unused when Resend is configured)
SMTP_HOST=
SMTP_PORT=587
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_FROM=
SMTP_USE_TLS=true

# Optional
DEBUG=false

# Service timeouts
DB_TIMEOUT_SECONDS=10
